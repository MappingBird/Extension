{
    "name": "MappingBird",
    "version": "0.9.5",
    "manifest_version": 2,
    "description": "__MSG_appDescription__",
    "icons": {
        "16": "images/icon-16.png",
        "128": "images/icon-128.png"
    },
    "default_locale": "en",
    "background": {
        "scripts": [
            "scripts/async.js",
            "scripts/background.js"
        ]
    },
    "browser_action": {
        "default_icon": {
            "19": "images/icon-16.png",
            "38": "images/icon-16.png"
        },
        "default_title": "mappingbird"
    },
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "css": [
                "styles/content.css"
            ],
            "js": [
                "scripts/utils.js",
                "scripts/contentscript.js"
            ],
            "run_at": "document_end",
            "all_frames": false
        }
    ],
    "permissions": [
        "tabs",
        "activeTab",
        "http://*/",
        "https://maps.google.com/*",
        "https://maps.googleapis.com/*",
        "http://localhost/*",
        "geolocation",
        "cookies",
        "storage"
    ],
    "web_accessible_resources": [
        "popup.html",
        "styles/*.css",
        "images/*",
        "scripts/*"
    ]
}
